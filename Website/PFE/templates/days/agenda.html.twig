{% extends 'layout/layoutDay.html.twig' %}
{% block body %}
<div class="limiter">
    <div class="wrapLimiter">
        {% for day in days %}
            <div class="day">
                <p class="dayName">{{day.name}}</p>
                <p class="dayDate">{{day.date}}</p>
                <p class="dayBegin">{{day.beginTime}}</p>
                <p class="dayEnd">{{day.endTime}}</p>
            </div>
        {% endfor %}
    </div>
</div>
{% endblock %}